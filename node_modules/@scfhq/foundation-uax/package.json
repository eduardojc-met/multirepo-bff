{
  "name": "@scfhq/foundation-uax",
  "version": "1.8.4",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:esm && npm run build:cjs && npm run css-compile && npm run fonts-copy",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "css-compile": "sass --style expanded --source-map --embed-sources --no-error-css src/themes/Tier1/:dist/css/",
    "fonts-copy": "cpy 'src/themes/fonts/*' dist/fonts",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "version": "node_modules/.bin/conventional-changelog -i CHANGELOG.md -o CHANGELOG.md -p angular && git add -A CHANGELOG.md",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.6.1",
    "@mui/lab": "^5.0.0-alpha.77",
    "@mui/material": "5.6.1",
    "@mui/styles": "5.6.1",
    "@mui/x-data-grid": "5.8.0",
    "@types/estree": "^0.0.51",
    "axios": "0.26.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^7.17.1",
    "react-router-dom": "5.3.0",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/react-router-dom": "^5.3.3",
    "conventional-changelog-cli": "^2.0.21",
    "conventional-recommended-bump": "^4.1.1",
    "copyfiles": "^2.4.1",
    "cpy-cli": "^4.1.0",
    "microbundle": "^0.14.2",
    "react-scripts": "^5.0.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "dist",
    "src"
  ],
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
